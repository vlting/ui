{
  "name": "@vlting/ui",
  "version": "0.2.0",
  "private": true,
  "description": "Cross-platform design system built on Tamagui v2 â€” layered, domain-free architecture",
  "license": "MIT",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "sideEffects": [
    "*.css"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "react-native": {
        "import": "./dist/esm/index.native.js"
      },
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    },
    "./config": {
      "types": "./dist/types/config/tamagui.config.d.ts",
      "import": "./dist/esm/config/tamagui.config.mjs",
      "require": "./dist/cjs/config/tamagui.config.cjs"
    },
    "./design-tokens": {
      "types": "./packages/design-tokens/index.ts",
      "import": "./packages/design-tokens/index.ts"
    },
    "./primitives": {
      "types": "./packages/primitives/index.ts",
      "import": "./packages/primitives/index.ts"
    },
    "./components": {
      "types": "./packages/components/index.ts",
      "import": "./packages/components/index.ts"
    },
    "./hooks": {
      "types": "./packages/hooks/index.ts",
      "import": "./packages/hooks/index.ts"
    },
    "./utils": {
      "types": "./packages/utils/index.ts",
      "import": "./packages/utils/index.ts"
    }
  },
  "workspaces": [
    ".",
    "examples/*"
  ],
  "scripts": {
    "build": "tamagui-build",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/types",
    "watch": "tamagui-build --watch",
    "clean": "tamagui-build clean",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "biome check src/ packages/",
    "lint:fix": "biome check --write src/ packages/",
    "format": "biome format --write src/ packages/",
    "typecheck": "tsc --noEmit",
    "dev:kitchen-sink": "yarn workspace @vlting/examples-kitchen-sink dev",
    "generate:icons": "node scripts/generate-icons.mjs"
  },
  "dependencies": {
    "@tamagui/animations-css": "2.0.0-rc.14",
    "@tamagui/animations-reanimated": "2.0.0-rc.14",
    "@tamagui/colors": "2.0.0-rc.14",
    "@tamagui/config": "2.0.0-rc.14",
    "@tamagui/font-inter": "2.0.0-rc.14",
    "@tamagui/lucide-icons": "2.0.0-rc.14",
    "@tamagui/shorthands": "2.0.0-rc.14",
    "@tamagui/theme-builder": "2.0.0-rc.14",
    "@tamagui/themes": "2.0.0-rc.14",
    "@tamagui/toast": "2.0.0-rc.14",
    "tamagui": "2.0.0-rc.14"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.2",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@biomejs/biome": "^1.9.4",
    "@tamagui/build": "2.0.0-rc.14",
    "@tanstack/react-table": "^8.21.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^16.3.2",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "prop-types": "^15.8.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-svg-web": "^1.0.9",
    "react-native-web": "^0.21.2",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "peerDependencies": {
    "@tanstack/react-table": ">=8",
    "expo-font": ">=13",
    "react": ">=19",
    "react-native": "*",
    "react-native-gesture-handler": ">=2.20",
    "react-native-reanimated": ">=4"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-table": {
      "optional": true
    },
    "react-native": {
      "optional": true
    },
    "react-native-reanimated": {
      "optional": true
    },
    "react-native-gesture-handler": {
      "optional": true
    },
    "expo-font": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vlting/ui.git"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
