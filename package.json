{
  "name": "@vlting/ui",
  "version": "0.1.0",
  "private": true,
  "description": "Cross-platform design system built on Tamagui v2",
  "license": "MIT",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "sideEffects": [
    "*.css"
  ],
