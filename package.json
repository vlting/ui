    "typescript": "~5.9.2"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-native": "*",
    "react-native-gesture-handler": ">=2.20",
    "react-native-reanimated": ">=4"
  },
  "peerDependenciesMeta": {
    "react-native": {
